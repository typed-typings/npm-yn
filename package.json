{
  "name": "@types/yn",
  "version": "1.2.0",
  "description": "Types for https://github.com/sindresorhus/yn",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\" -e bundle.d.ts",
    "bundle": "typings bundle -o bundle.d.ts",
    "exec": "ts-node test.ts",
    "test": "npm run lint && npm run bundle && npm run exec",
    "prepublish": "typings install",
    "postpublish": "greenkeeper-postpublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/types/npm-yn.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/types/npm-yn/issues"
  },
  "homepage": "https://github.com/types/npm-yn#readme",
  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "ts-node": "^1.6.1",
    "tslint": "^3.15.1",
    "tslint-config-typings": "^0.2.4",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "yn": "~1.2.0"
  }
}
